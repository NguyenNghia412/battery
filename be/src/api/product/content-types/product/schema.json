{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "nameLong": {
      "type": "text"
    },
    "description": {
      "type": "richtext"
    },
    "price": {
      "type": "decimal"
    },
    "product_category": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::product-category.product-category",
      "inversedBy": "products"
    },
    "thumbnail": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "code": {
      "type": "string"
    }
  }
}
